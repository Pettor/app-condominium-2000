@model IEnumerable<Condominium2000.Models.Announcement>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            TitleSv
        </th>
        <th>
            TitleEn
        </th>
        <th>
            SubTitleSv
        </th>
        <th>
            SubTitleEn
        </th>
        <th>
            DateCreated
        </th>
        <th>
            ContentSv
        </th>
        <th>
            ContentEn
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TitleSv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TitleEn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubTitleSv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubTitleEn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateCreated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContentSv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContentEn)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {id = item.Id}) |
                @Html.ActionLink("Details", "Details", new {id = item.Id}) |
                @Html.ActionLink("Delete", "Delete", new {id = item.Id})
            </td>
        </tr>
    }

</table>